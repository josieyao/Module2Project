<table class="table table-striped table-hover table-info ">
    <thead class="thead-dark">
        <tr>
        <th scope="col">#</th>
        <th scope="col">Level</th>
        <th scope="col">Donation amount</th>
        <th scope="col">reward</th>
        <th scope="col">Number of Donors</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">1</th>
            <td>Bronze</td>
            <td>Less than $100</td>
            <td>None</td>
            <td><%= @bronze_users.count %></td>
        </tr>
        <tr>
            <th scope="row">2</th>
            <td>Silver</td>
            <td>Between $250 and $500</td>
            <td>$5 gift card</td>
            <td><%= @silver_users.count %></td>
        </tr>
        <tr>
            <th scope="row">3</th>
            <td>Gold</td>
            <td>Between $250 and $500</td>
            <td>$20 gift card</td>
            <td><%= @gold_users.count %></td>
        </tr>
        <tr>
            <th scope="row">4</th>
            <td>Platinum</td>
            <td>More than $500</td>
            <td>$50 gift card</td>
            <td><%= @platinum_users.count %></td>
        </tr>
    </tbody>
</table>
